{
  "redirects": [
    {
      "source": "/:path*",
      "has": [
        {
          "type": "query",
          "key": "m",
          "value": "1"
        }
      ],
      "destination": "/:path*",
      "permanent": true
    },
    {
      "source": "/:path*",
      "has": [
        {
          "type": "host",
          "value": "buanacoding.com"
        }
      ],
      "destination": "https://www.buanacoding.com/:path*",
      "permanent": true
    }
  ],
  "trailingSlash": true
}
